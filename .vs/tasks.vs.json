{
    "version": "0.2.1",
    "tasks": [
        {
            "taskName": "Run",
            "appliesTo": "*",
            "type": "launch",
            "command": "${env.COMSPEC}",
            "args": [
                "PATH=C:\\cygwin64\\bin;%PATH% && c:\\cygwin64\\bin\\bash /cygdrive/c/Users/abhis/code/clibdocker/run_cygwin_clibdocker.sh"
            ]
        },
        {
            "taskName": "Build",
            "appliesTo": "*.c",
            "contextType": "build",
            "type": "launch",
            "command": "${env.COMSPEC}",
            "args": [
                "PATH=C:\\cygwin64\\bin;%PATH% && c:\\cygwin64\\bin\\bash -c \"make -f Makefile.win64 all\""
            ]
        },
        {
            "taskName": "Clean",
            "appliesTo": "*",
            "contextType": "clean",
            "type": "default",
            "command": "${env.COMSPEC}",
            "args": [
                "PATH=C:\\cygwin64\\bin;%PATH% && c:\\cygwin64\\bin\\bash -c \"make clean -f Makefile.win64\""
            ]
        }
    ]
}