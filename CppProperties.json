{
  "environments": [
    {
      "MINGW_PREFIX": "C:/cygwin64",
      "path": "${env.path};${env.MINGW_PREFIX}/bin;${env.MINGW_PREFIX}/local/bin"
    }
  ],
  "configurations": [
    {
      "inheritEnvironments": [
        "msvc_x64"
      ],
      "name": "x64-Release",
        "includePath": [
            "${env.MINGW_PREFIX}/usr/include/",
            "${env.MINGW_PREFIX}/usr/include/json-c",
            "${env.INCLUDE}",
            "${workspaceRoot}\\**"
        ],
      "defines": [
        "WIN32",
        "NDEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "intelliSenseMode": "windows-clang-x64"
    },
    {
      "inheritEnvironments": [
        "msvc_x64"
      ],
      "name": "x64-Debug",
        "includePath": [
            "${env.MINGW_PREFIX}/usr/include/",
            "${env.MINGW_PREFIX}/usr/include/json-c",
            "${env.INCLUDE}",
            "${workspaceRoot}\\**"
        ],
      "defines": [
        "WIN32",
        "NDEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "intelliSenseMode": "windows-clang-x64"
    }
  ]
}